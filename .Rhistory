data_2 <- c(16,20,19,25)
data_3 <- c(17,15,18,20)
par(mfrow=c(2,4)) # all plots on one page
opts = c("l","o")
length(opts)
for(i in 1:length(opts)){
heading = paste("type= (20BCE0480) ",opts[i])
plot(month, data_1, type="n", xlab = "Month", ylab = "Rainfall in mm", main=heading)
legend("bottomright",1,20,legend=c("2017","2018","2019"), cex=0.8, col=c("red", "blue"),lty=1:2)
lines(month, data_1, type=opts[i],lty=1,col="red")
lines(month,data_2, type=opts[i],lty=2,col="blue")
lines(month, data_3, type=opts[i],lty=3,col="green")
}
